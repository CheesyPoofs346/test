{
  "rewrites": [
    { "source": "/popup.html", "destination": "/popup.html" } // Ensure popup.html is served correctly
  ],
  "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" } // Important for popups in some browsers
        ]
      }
    ]
}
